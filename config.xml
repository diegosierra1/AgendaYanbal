<?xml version='1.0' encoding='utf-8'?>
<widget id="com.agendamia.yanbal" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Agenda Yanbal</name>
    <description>
        Agenda para Empresarios Yanbal
    </description>
    <author email="soportev@indexdream.com" href="http://indexdream.com">
        Index Dream
    </author>
    <content src="index.html" />
    <access launch-external="yes" origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" height="36" platform="android" src="icon/36x36.png" width="36" />
    <icon density="mdpi" height="48" platform="android" src="icon/48x48.png" width="48" />
    <icon density="hdpi" height="72" platform="android" src="icon/72x72.png" width="72" />
    <icon density="xhdpi" height="96" platform="android" src="icon/96x96.png" width="96" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
