<?xml version='1.0' encoding='utf-8'?>
<widget id="com.agendamia.yanbal" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Agenda Yanbal</name>
    <description>
        Agenda para Empresarios Yanbal
    </description>
    <author email="soportev@indexdream.com" href="http://indexdream.com">
        Index Dream
    </author>
    <content src="index.html" />
    <!--
    <access origin="*" />
    -->
    <access origin="*" launch-external="yes" />
    <access origin="mailto:*" launch-external="yes"/>
    <access origin="tel:*" launch-external="yes"/>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
<icon platform="android" src="icon/36x36.png" density="ldpi" width="36" height="36"/>
<icon platform="android" src="icon/48x48.png" density="mdpi" width="48" height="48"/>
<icon platform="android" src="icon/72x72.png" density="hdpi" width="72" height="72"/>
<icon platform="android" src="icon/96x96.png" density="xhdpi" width="96" height="96"/>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="cordova-plugin-contacts" spec="~3.0.1" />
    
